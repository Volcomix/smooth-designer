{
  "name": "smooth-designer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint .",
    "flow": "flow",
    "test": "react-scripts test --env=jsdom",
    "fulltest": "clear && yarn lint && yarn flow --include-warnings && yarn test --verbose --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ],
    "coverageReporters": [
      "html",
      "text",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3-force": "^1.1.0",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint-plugin-prettier": "^2.4.0",
    "flow-bin": "^0.61.0",
    "prettier": "^1.9.2",
    "redux-mock-store": "^1.4.0"
  }
}
